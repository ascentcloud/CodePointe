{
    "name": "code-pointe",
    "displayName": "CodePointe",
    "description": "Deploys Salesforce files on save",
    "version": "1.0.3",
    "publisher": "geopointe",
    "engines": {
        "vscode": "^1.28.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/geopointe/CodePointe.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "codePointe.compileProject",
                "title": "CodePointe: Compile Project"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "eslint": "^4.11.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.30"
    },
    "dependencies": {
        "bluebird": "^3.5.3",
        "lodash": "^4.17.11"
    }
}
